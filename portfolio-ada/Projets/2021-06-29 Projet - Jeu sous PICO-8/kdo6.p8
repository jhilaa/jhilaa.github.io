function inittext()
	text={}
	text.active=true
	text.str={}
	text.str={"hello","ca va?"}
end

function drawtext()
local x=20
local y=48
local w=108
local h= 80

--draw text box
rectfill(x-1,y-2,w+2,h+2,5)
rectfill(x,y,w,h,6)

	--write text
	for i=1,#text.str do
	
		local txt=text.str[i]
		local tx=x+1
		local ty=y-5+(i*6)
	
		print (txt,tx,ty,5)
	end
end


