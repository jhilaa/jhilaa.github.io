cls()
draw_map()

	drawtext()
	
	local sprite=sprites[frameindex]
	sspr(sprite.x,sprite.y,sprite.w,sprite.h,56+sprite.xo,56+sprite.yo,sprite.w,sprite.h,sprite.flipx)
 local sprite2=sprites2[frameindex]
	sspr(sprite2.x,sprite2.y,sprite2.w,sprite2.h,56+sprite2.xo,56+sprite2.yo,sprite2.w,sprite2.h,sprite2.flipx)
	--coeur
